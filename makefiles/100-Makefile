CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm -f
CFLAGS = -Wall -Werror -Wextra -pedantic
OBJ = $(patsuo, $(SRC))
.PHONY = all oclean clean re fclean

all: m.h $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

oclean: 
	$(RM) $(OBJ)

clean:
	$(RM) *~ $(NAME)

fclean: oclean clean

re: oclean all
